{
  "name": "Schoology Albums Downloader",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "Save Schoology media albums with a single click",
  "author": "1nm",
  "background": {
    "service_worker": "service-worker.js"
  },
  "icons": {
    "128": "images/icon128.png",
    "48": "images/icon48.png",
    "16": "images/icon16.png"
  },
  "permissions": ["tabs", "scripting"],
  "host_permissions": ["https://*.schoology.com/album/*"],
  "action": {}
}
